PROTO INT division(k)

// Programme de devision qui nous ren comme r√©sultat si le n est divisible par 2
FUNC VOID main() {
	INT n, y
	PRINT "Input n between 0 and 11:\n"
	READ n
    division(n,2)
}

FUNC VOID division(x,y)
{
	INT i
	i := 0
	READ x
	READ y
	WHILE x-y DO
	{
		x := x-y
		i := i+1
	}
	DONE

	IF i-10 THEN
	PRINT "La disivion de ", x, " par ", y, " est superieur a 10 "
	ELSE
	PRINT "La disivion de ", x, " par ", y, " est inferieur a 10 "
	FI
}
